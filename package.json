{
    "name": "tidev.io",
    "version": "2.1.0",
    "private": true,
    "scripts": {
        "build": "pnpm copy-pdf-files && pnpm run build:css && next build",
        "build:css": "tailwindcss -m -i styles/site.css -o app/styles/site.css",
        "copy-pdf-files": "node scripts/copy-pdf-files",
        "dev": "pnpm copy-pdf-files && concurrently -k \"pnpm dev:css\" \"next dev\"",
        "dev:css": "tailwindcss -w -i styles/site.css -o public/styles/site.css",
        "start": "next start",
        "lint": "next lint"
    },
    "dependencies": {
        "@google-cloud/storage": "6.11.0",
        "formidable": "3.5.0",
        "fs-extra": "11.1.1",
        "globby": "13.2.0",
        "gray-matter": "4.0.3",
        "markdown-it": "13.0.1",
        "next": "13.4.7",
        "next-auth": "4.22.1",
        "next-seo": "6.1.0",
        "open-pdf-sign": "0.1.0",
        "pdf-lib": "1.17.1",
        "pdfjs-dist": "3.7.107",
        "querystring": "0.2.1",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-signature-canvas": "1.0.6",
        "uuid": "9.0.0"
    },
    "devDependencies": {
        "@tailwindcss/typography": "0.5.9",
        "@types/formidable": "3.4.0",
        "@types/lodash": "4.14.195",
        "@types/markdown-it": "12.2.3",
        "@types/node": "20.3.2",
        "@types/react": "18.2.14",
        "@types/react-pdf": "6.2.0",
        "@types/react-signature-canvas": "1.0.2",
        "@types/uuid": "9.0.2",
        "autoprefixer": "10.4.14",
        "concurrently": "8.2.0",
        "eslint": "8.43.0",
        "eslint-config-next": "13.4.7",
        "postcss": "8.4.24",
        "tailwindcss": "3.3.2",
        "typescript": "5.1.6"
    },
    "engines": {
        "node": ">=16"
    }
}
