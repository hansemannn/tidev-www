{
    "name": "tidev.io",
    "version": "2.1.0",
    "private": true,
    "scripts": {
        "build": "npm run build:css && next build",
        "build:css": "tailwindcss -m -i styles/site.css -o app/styles/site.css",
        "dev": "concurrently \"npm run dev:css\" \"next dev\"",
        "dev:css": "tailwindcss -w -i styles/site.css -o public/styles/site.css",
        "start": "next start",
        "lint": "next lint"
    },
    "dependencies": {
        "@google-cloud/storage": "6.9.5",
        "@react-pdf/renderer": "3.1.9",
        "globby": "13.1.3",
        "gray-matter": "4.0.3",
        "markdown-it": "13.0.1",
        "next": "13.3.0",
        "next-auth": "4.21.1",
        "next-seo": "6.0.0",
        "querystring": "0.2.1",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "uuid": "9.0.0"
    },
    "devDependencies": {
        "@tailwindcss/typography": "0.5.9",
        "@types/markdown-it": "12.2.3",
        "@types/node": "18.15.11",
        "@types/react": "18.0.33",
        "@types/uuid": "9.0.1",
        "autoprefixer": "10.4.14",
        "concurrently": "8.0.1",
        "eslint": "8.37.0",
        "eslint-config-next": "13.3.0",
        "postcss": "8.4.21",
        "tailwindcss": "3.3.1",
        "typescript": "5.0.3"
    },
    "engines": {
        "node": ">=16"
    }
}
